{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "skipLibCheck": true,
      "noImplicitAny": false,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      
    },
    "lib": ["es2015"]
  }
  