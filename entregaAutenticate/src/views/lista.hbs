<div class="row">
    <div class="col-md-12">
        <table class="table">
            <thead class="thead-dark">
                <tr>
                <th scope="col">Id</th>
                <th scope="col">Nombre</th>
                <th scope="col">Precio</th>
                <th scope="col">Foto</th>
                </tr>
            </thead>
            <tbody>
                {{#each productos}}
                <tr>
                <td>{{this.id}}</td>
                <td>{{this.nombre}}</td>
                <td>{{this.precio}}</td>
                <td><img src='{{this.thumbnail}}' width="50"></td>
                
                
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>
